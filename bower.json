{
  "name": "kbwood_countdown",
  "version": "2.0.2",
  "description": "This plugin sets a div or span to show a countdown to a given time.",
  "keywords": [
    "countdown",
    "i18n",
    "timer",
    "timezone",
    "ui"
  ],
  "license": "MIT",
  "authors": [
    "Keith Wood"
  ],
  "main": "jquery.countdown.js",
  "ignore": [
    "*.gif",
    "*.html",
    "*.json",
    "*.md",
    "*.png",
    "jquery.countdown-*.js",
    "jquery.*.min.js"
  ],
  "dependencies": {
    "jquery": ">=1.4"
  }
}
